    "E2 = annual_ret_summ.loc['Media','Japon']\n",
    "# Volatilidades individuales\n",
    "s1 = annual_ret_summ.loc['Volatilidad','EU']\n",
    "s2 = annual_ret_summ.loc['Volatilidad','Japon']\n",
    "# Correlacion\n",
    "r12 = corr.loc['EU','Japon']\n",
    "E1, E2, s1, s2, r12"
